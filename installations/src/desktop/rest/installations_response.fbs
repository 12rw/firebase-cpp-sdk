namespace firebase.installations.fbs;

table AuthTokenInfo {
  token:string (id: 0);
  expiresIn:string (id: 1);
}

table Installations {
  name:string (id: 0);
  fid:string (id: 1);
  refreshToken:string (id: 2);
  authToken:AuthTokenInfo (id: 3);
}

root_type Installations;

